# by default if hots config is not listed here ssh will be taking ssh key named
# 'id_ed25519' (ie when iteracting via git with private repo)

Host aur.archlinux.org
  IdentityFile ~/.ssh/aur
  User aur

Host archive
  Hostname 192.168.1.81
  IdentityFile ~/.ssh/sarvar
  User earthian
  RemoteCommand $SHELL
  RequestTTY yes
  AddKeysToAgent yes
