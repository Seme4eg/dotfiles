
(setq ewal-base-palette
      '((comment    . "{color8}") ;; unofficial pywal comment color
        (background . "{background}")
        (foreground . "{foreground}")
        (cursor     . "{cursor}")
        ;; these 8 are tty colors that can be found in 'tty-defined-color-alist'
        (black      . "{background}")
        (red        . "{color1}")
        (green      . "{color2}")
        (yellow     . "{color3}")
        (blue       . "{color4}")
        (magenta    . "{color5}")
        (cyan       . "{color6}")
        (white      . "{color7}")))
